{
  "paths": {
    "/login4": {
      "post": {
        "summary": "执行登录",
        "description":
          "登录完成之后应该获取跟loginStatus一样的响应；mock时用户名为error就登录失败",
        "tags": ["login"],
        "consumes": [
          "application/x-www-form-urlencoded",
          "multipart/form-data"
        ],
        "parameters": [
          {
            "name": "username",
            "in": "formData",
            "type": "string",
            "required": true
          },
          {
            "name": "password",
            "in": "formData",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "cookies": {
              "name": {
                "type": "string",
                "default": {
                  "__script": "username"
                }
              }
            },
            "schema": {
              "type":"string",
              "default": {
                "__script": "username"
              }
            }
          },
          "240": {
            "description": "无效的登录",
            "condition": {
              "script": "username==='error'"
            }
          }
        }
      }
    },
    "/test9": {
      "get": {
        "summary": "test",
        "description": "description",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "ok",
            "headers": {
              "n1": {
                "type": "string",
                "default": {
                  "__script": "id"
                }
              }
            },
            "cookies": {
              "n2": {
                "type": "string",
                "default": {
                  "__script": "id"
                }
              }
            }
          }
        }
      }
    },
    "/users":{
      "get":{
        "summary": "获取登录状态",
        "description": "包含权限信息",
        "parameters": [
          {
            "name": "current",
            "in": "query",
            "type": "number",
            "required": false
          },
          {
            "name": "pageSize",
            "in": "query",
            "type": "number",
            "required": false
          }
        ],
        "responses":{
          "200":{
            "description":"正常结果",
            "schema":{
              "type":"object",
              "required":[
                "list","pagination"
              ],
              "properties":{
                "list": {
                  "type":"array",
                  "items":{
                    "$ref":"#/definitions/UserModel"
                  },
                  "maxItems":{
                    "__script":"pageSize||20"
                  },
                  "minItems":{
                    "__script":"pageSize||20"
                  }
                },
                "pagination":{
                  "type":"object",
                  "required":[
                    "current","pageSize","total"
                  ],
                  "properties":{
                    "current":{
                      "type":"integer",
                      "description":"当前页数",
                      "default":{
                        "__script":"current?parseInt(current):1"
                      }
                    },
                    "pageSize":{
                      "type":"integer",
                      "description":"每页大小",
                      "default":{
                        "__script":"pageSize?parseInt(pageSize):20"
                      }
                    },
                    "total":{
                      "type":"integer",
                      "description":"总数据量",
                      "default":100000
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "UserModel":{
      "type":"object",
      "required":[
        "id","loginName","role"
      ],
      "properties":{
        "id":{
          "type":"number",
          "description":"用户主键"
        },
        "loginName":{
          "type":"string",
          "description":"用户登录名"
        },
        "role":{
          "type":"string",
          "description":"角色"
        }
      }
    }
  }
}